WITH CustomerPurchases AS (
    SELECT 
        CustomerId,
        COUNT(DISTINCT InvoiceId) AS Purchase_Count
    FROM invoices
    GROUP BY CustomerId
)
SELECT 
    ROUND(
        CAST(SUM(CASE WHEN Purchase_Count &gt; 1 THEN 1 ELSE 0 END) AS FLOAT) * 100.0 / 
        COUNT(*), 2
    ) AS Repeat_Purchase_Ratio
FROM CustomerPurchases;</sql><current_tab id="0"/></tab_sql></sqlb_project>
